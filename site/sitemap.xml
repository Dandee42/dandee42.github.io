---
layout: null
sitemap: false
---
<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

  {% assign build_time = site.time | date: '%Y-%m-%dT%H:%M:%S%:z' %}

  {% for lang in site.languages %}
    <sitemap>
      {% if lang == site.default_lang %}
        <loc>{{ site.url }}{{ site.baseurl }}/sitemap_cs.xml</loc>
      {% elsif lang == "en" %}
        <loc>{{ site.url }}{{ site.baseurl }}/sitemap_en.xml</loc>
      {% endif %}
      <lastmod>{{ build_time }}</lastmod>
    </sitemap>
  {% endfor %}

</sitemapindex>